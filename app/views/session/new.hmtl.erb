<h1>Hello</h1>
<% if @error %>
 <%= @error %>
<% end %>


<%= form_tag "/login" do  %>
  <p>
   <%= label :email, "Email" %><br>
   <%= text_field_tag :email %>
  </p>

  <p>
   <%= label :password, "Password" %><br>
   <%= password_field_tag :password %>
  </p>

  <p>
   <%= submit_tag "submit" %>
  </p>
<% end %>
