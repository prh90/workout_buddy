<h1 class="title_web">Workout Buddy: Login</h1><br>

<div class="container">
<%= form_tag '/login', method: 'post', :class => 'form-inline' do  %>
  <div class="form-group">
    <%= label_tag 'email', 'Email'%><br>
    <%= text_field_tag :email,params[:email], class: 'form-control', autofocus: true %>
  </div>
  <div class="form-group">
    <%= label_tag 'password',"Password" %><br>
    <%= password_field_tag :password,nil, class: 'form-control'  %>
  </div>
  <br><%= submit_tag "Login", class: 'btn btn-success' %>
<%end%>
</div>
